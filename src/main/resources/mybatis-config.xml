<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias type="com.example.demo.TEST_001.controller.IndexController" alias="IndexController"></typeAlias>

        <!-- 식재료 관리 시스템 -->
        <typeAlias type="com.example.demo.TEST_001.dto.IngredientDTO" alias="ingredient"></typeAlias>
        <typeAlias type="com.example.demo.TEST_001.dto.CategoryDTO" alias="category"></typeAlias>
        <typeAlias type="com.example.demo.TEST_001.dto.IngredientDefaultExpiryDTO" alias="ingredientDefaultExpiry"></typeAlias>

        <!-- 사용자 관리 -->
        <typeAlias type="com.example.demo.TEST_001.dto.UserDTO" alias="user"></typeAlias>

        <!-- 알림 시스템 -->
        <typeAlias type="com.example.demo.TEST_001.dto.NotificationDTO" alias="notification"></typeAlias>

    </typeAliases>
</configuration>